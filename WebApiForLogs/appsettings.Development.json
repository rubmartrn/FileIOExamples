{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "WebApiForLogs.Controllers.First": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "WebApiForLogs.Controllers.First": "Warning"
      }
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  }
}
